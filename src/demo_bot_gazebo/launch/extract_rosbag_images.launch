<launch>
    <node pkg="rosbag" type="play" name="rosbag" required="true" args="$(find demo_bot_gazebo)/bag/white_gate.bag"/>
    <node name="extract" pkg="image_view" type="extract_images" respawn="false" required="true" output="screen" cwd="ROS_HOME">
        <param name="sec_per_frame" value="0.033" /> 
        <!-- The sec_per_frame is used to load the number of frames per min, keep it in sync with the rosbag loading frames speed -->
      <remap from="image" to="/front_camera/image_raw"/>
      <!-- note the topic name, it is the topic your rosbag file is running at -->
    </node>
  </launch>