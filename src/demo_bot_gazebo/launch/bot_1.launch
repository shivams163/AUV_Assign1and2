<launch>
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find demo_bot_gazebo)/urdf/bot_1.xacro' " />

    <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" output="screen" respawn="false"
    args="-urdf -param robot_description -model bot_1 -x 0 -y -1 -z 0" />
</launch>